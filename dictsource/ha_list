abu      $u   // it, thing
ba      $u   // is-not
ita      $u   // she, it
shi      $u   // he, it
su      $u   // they,them
ku       $u+   // them
mu       $u+   // we, us
kai      $u   // you male, head
ke      $u   // you female
ni     $u   // me, it

tare dani    $u+ // with me
tare dakai    $u // with you
tare dashi    $u+ // with him
tare da'ita    $u+ // with her
tare damu    $u+ // with us
da kai   $u+ // and you
tare dasu     $u+ // with them
tare daku    $u+ // with you all

